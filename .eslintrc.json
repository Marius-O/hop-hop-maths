{
  "root": true,
  "extends": [
    "airbnb",
    "airbnb-typescript",
    "prettier"
  ],
  "env": {
    "browser": true
  },
  "parserOptions": {
    "project": [
      "./tsconfig.json",
      "./tsconfig.eslint.json"
    ]
  },
  "rules": {
    "react/jsx-uses-react": "off",
    "react/react-in-jsx-scope": "off"
  }
}
